{
	"compilerOptions": {
    "incremental": true, 
    "target": "ESNext",
    "module": "NodeNext",
    "declaration": true, 
    "outDir": "./dist",
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
	},
	"include": ["src/**/*", "eleventy.ts"],
	"exclude": ["node_modules", "dist"]
}